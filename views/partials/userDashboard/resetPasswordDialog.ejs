<div id="resetPasswordDialog" class="modal fade" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">

            <div class="modal-header">
                <h5 class="modal-title">Edit Address</h5>
            </div>


            <div class="modal-body">

                New Password<br>
                <input id="newPasswordInput" type="password" name="password"><br>

                Confirm New Password<br>
                <input id="confirmNewPasswordInput" type="password" name="password"><br>
            </div>

            <div class="modal-footer">
                <button class="btn btn-secondary" data-dismiss="modal" type="button">Close</button>
                <input id="resetPasswordBtn" type="submit" value="Submit" class="btn btn-primary"/>
            </div>
        </div>
    </div>
</div>